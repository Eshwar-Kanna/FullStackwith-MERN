                        import React from "react";
                        import  ReactDOM  from "react-dom/client";


                        import {Mybutton , Mybuttongroup  , Mycheckbox , Mytextfield} from "./App";


                        const root = ReactDOM.createRoot(document.getElementById("root"))

                        root.render(
                          <div>
                          <Mybutton />
                          <br />
                          <Mybuttongroup />
                          <Mycheckbox />

                          <br />
                          <Mytextfield />
                         </div>
                        )
                        
                        
                        
                        
                        
                        
                        
