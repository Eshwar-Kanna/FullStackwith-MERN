𝐢𝐧𝐝𝐞𝐱.𝐣𝐬 


            import React from 'react';
            import ReactDOM from 'react-dom'
            import App from './App';
            import  {lw , kanna}   from "./App.js"

            var lname = "rebal star"
            var fname = "prabhas"
            var date = new Date()
            var myyear = date.getFullYear()
            var month = date.getMonth()
            var  color = {color:'pink'}
            var image = "https://th.bing.com/th/id/OIP.ttIaAn8vVWsgTIk1IkpoyAHaLH?pid=ImgDet&rs=1"
            ReactDOM.render(
            <div> 
              <div>{lw()}</div>
              <p><img width = "100px"  height = "100px"  src = {image} /></p>
              <p> Iam MR {fname + " " + lname}</p>
              <a  href = "" > click here </a>
              <a style = {color}> the current month {month} and Year {myyear}</a>
              <div> {kanna()}</div>
             </div> ,
             document.getElementById("root")
            );
            
            
            
            
            
